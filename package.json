{
  "name": "big-quest-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install && yarn build",
    "dev": "next dev",
    "mock": "yarn mcs -w",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "pretty": "yarn format && yarn lint --fix && yarn stylelint --fix",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6010"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --no-error-on-unmatched-pattern --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.17.15",
    "@tanstack/react-query-devtools": "^5.17.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cookies-next": "^4.1.0",
    "lucide-react": "^0.312.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-intl": "^6.6.1",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.4",
    "@siberiacancode/eslint": "^1.0.3",
    "@siberiacancode/prettier": "^1.0.1",
    "@siberiacancode/stylelint": "^1.0.2",
    "@storybook/addon-a11y": "^7.6.6",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/nextjs": "7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/theming": "^7.6.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mock-config-server": "^3.3.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "storybook": "^7.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
